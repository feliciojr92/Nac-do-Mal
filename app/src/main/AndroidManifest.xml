        <activity
                android:name=".MainNetShoesActivity"
                android:parentActivityName=".MainActivity">
        </activity>
        <activity
                android:name=".MainMagaluActivity"
                android:parentActivityName=".MainActivity">
        </activity>
        <activity
                android:name=".MainWebMotorsActivity"
                android:parentActivityName=".MainActivity">
        </activity>
        <activity
                android:name=".MainEBayActivity"
                android:parentActivityName=".MainActivity">
        </activity>
        <activity
                android:name=".MainBuscapeActivity"
                android:parentActivityName=".MainActivity">
        </activity>
        <activity
                android:name=".MainMercadoLivreActivity"
                android:parentActivityName=".MainActivity">
        </activity>
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
    </application>

</manifest>